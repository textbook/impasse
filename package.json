{
  "name": "impasse",
  "version": "0.8.8",
  "description": "Generate moderately secure passwords",
  "private": true,
  "scripts": {
    "prebuild": "npm run cleanup",
    "build": "npm run build:server && npm run build:client",
    "build:client": "webpack --config client/webpack/prod.config.js",
    "build:server": "babel server --out-dir dist --ignore \"**/*.test.js\"",
    "cleanup": "rimraf ./dist/*",
    "predev": "npm run cleanup",
    "dev": "concurrently -k -n \"client,build,serve\" \"npm run dev:client\" \"npm run dev:build\" \"npm run dev:serve\"",
    "dev:build": "npm run build:server -- --source-maps --watch",
    "dev:client": "webpack serve --config client/webpack/dev.config.js",
    "dev:serve": "wait-on -l file:dist/server.js && cross-env PORT=3100 nodemon dist/server.js --watch dist",
    "predocker": "docker build . -t textbook/impasse --build-arg NODE_RELEASE=$(cat .nvmrc)",
    "docker": "docker run -e NODE_ENV=development -p 3000:80 textbook/impasse",
    "e2e": "concurrently -k -s first -n \"app,e2e\" \"npm run serve\" \"npm run e2e:safe\"",
    "e2e:dev": "cypress open",
    "e2e:docker": "concurrently -k -s first -n \"app,e2e\" \"npm run docker\" \"npm run e2e:safe\"",
    "e2e:run": "cypress run",
    "e2e:safe": "wait-on -l http-get://localhost:3000 && npm run e2e:run",
    "lint": "eslint .",
    "preserve": "npm run build",
    "serve": "npm start",
    "ship": "npm run lint && npm run test && npm run e2e",
    "start": "node dist/server.js",
    "test": "jest",
    "test:watch": "npm run test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/textbook/impasse.git"
  },
  "author": {
    "name": "Jonathan Sharpe",
    "email": "mail@jonrshar.pe",
    "url": "http://hello.jonrshar.pe"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/textbook/impasse/issues"
  },
  "homepage": "https://github.com/textbook/impasse#readme",
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.5",
    "@babel/eslint-parser": "^7.16.5",
    "@babel/plugin-proposal-class-properties": "^7.16.5",
    "@babel/plugin-transform-runtime": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "@babel/preset-react": "^7.16.5",
    "@codeyourfuture/eslint-config-standard": "^3.0.2",
    "@testing-library/cypress": "^8.0.2",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "axe-core": "^4.3.5",
    "axios": "^0.24.0",
    "babel-loader": "^8.2.3",
    "bulma": "^0.9.3",
    "concurrently": "^6.5.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.5.1",
    "cypress": "^9.1.1",
    "cypress-axe": "^0.13.0",
    "cypress-multi-reporters": "^1.5.0",
    "eslint": "^8.4.1",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-jest": "^25.3.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "html-webpack-tags-plugin": "^3.0.2",
    "jest": "^27.4.5",
    "jest-junit": "^13.0.0",
    "msw": "^0.36.3",
    "nodemon": "^2.0.15",
    "prop-types": "^15.7.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "rimraf": "^3.0.0",
    "sass": "^1.45.0",
    "sass-loader": "^12.4.0",
    "style-loader": "^3.3.1",
    "supertest": "^6.1.6",
    "wait-on": "^6.0.0",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.6.0",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.16.5",
    "express": "^4.17.2",
    "helmet": "^4.6.0",
    "hibp": "^11.0.0",
    "morgan": "^1.9.1",
    "swagger-jsdoc": "^6.1.0",
    "swagger-ui-express": "^4.3.0",
    "word-list": "^3.1.0"
  },
  "engines": {
    "node": "16.13.1",
    "npm": "8.1.2"
  }
}
