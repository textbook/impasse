{
  "name": "impasse",
  "version": "0.10.11",
  "description": "Generate moderately secure passwords",
  "private": true,
  "scripts": {
    "prebuild": "npm run cleanup",
    "build": "npm run build:server && npm run build:client",
    "build:client": "webpack --config client/webpack/prod.config.js",
    "build:server": "babel server --out-dir dist --ignore \"**/*.test.js\"",
    "cleanup": "rimraf ./dist/*",
    "predev": "npm run cleanup",
    "dev": "concurrently -k -n \"client,server\" \"npm run dev:client\" \"npm run dev:server\"",
    "dev:build": "npm run build:server -- --source-maps --watch",
    "dev:client": "webpack serve --config client/webpack/dev.config.js",
    "dev:server": "cross-env LOG_LEVEL=debug PORT=3100 nodemon --exitcrash --inspect --delay 500ms --watch server --exec babel-node server/server.js",
    "predocker": "docker build . -t textbook/impasse --build-arg NODE_RELEASE=$(cat .nvmrc)",
    "docker": "docker run -e NODE_ENV=development -p 3000:80 textbook/impasse",
    "e2e": "concurrently -k -s first -n \"app,e2e\" \"npm run serve\" \"npm run e2e:safe\"",
    "e2e:dev": "cypress open",
    "e2e:docker": "concurrently -k -s first -n \"app,e2e\" \"npm run docker\" \"npm run e2e:safe\"",
    "e2e:run": "cypress run",
    "e2e:safe": "wait-on -l http-get://localhost:3000 && npm run e2e:run",
    "lint": "eslint .",
    "preserve": "npm run build",
    "serve": "npm start",
    "ship": "npm run lint && npm run test && npm run e2e",
    "start": "node dist/server.js",
    "test": "jest",
    "test:watch": "npm run test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/textbook/impasse.git"
  },
  "author": {
    "name": "Jonathan Sharpe",
    "email": "mail@jonrshar.pe",
    "url": "http://hello.jonrshar.pe"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/textbook/impasse/issues"
  },
  "homepage": "https://github.com/textbook/impasse#readme",
  "devDependencies": {
    "@babel/cli": "^7.18.6",
    "@babel/core": "^7.18.6",
    "@babel/eslint-parser": "^7.18.2",
    "@babel/node": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@babel/preset-react": "^7.18.6",
    "@codeyourfuture/eslint-config-standard": "^3.0.3",
    "@testing-library/cypress": "^8.0.3",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.2.1",
    "axe-core": "^4.4.2",
    "axios": "^0.27.2",
    "babel-loader": "^8.2.5",
    "bulma": "^0.9.4",
    "clsx": "^1.1.1",
    "concurrently": "^7.2.2",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "cypress": "^10.3.0",
    "cypress-axe": "^1.0.0",
    "cypress-multi-reporters": "^1.6.1",
    "eslint": "^8.19.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-jest": "^26.5.3",
    "eslint-plugin-jsx-a11y": "^6.6.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "html-webpack-tags-plugin": "^3.0.2",
    "jest": "^28.1.2",
    "jest-environment-jsdom": "^28.1.2",
    "jest-junit": "^14.0.0",
    "msw": "^0.43.0",
    "nodemon": "^2.0.18",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^3.0.2",
    "sass": "^1.53.0",
    "sass-loader": "^13.0.2",
    "style-loader": "^3.3.1",
    "supertest": "^6.2.4",
    "wait-on": "^6.0.1",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.3",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.18.6",
    "express": "^4.18.1",
    "helmet": "^5.1.0",
    "hibp": "^11.1.0",
    "morgan": "^1.10.0",
    "swagger-jsdoc": "^6.2.1",
    "swagger-ui-express": "^4.4.0",
    "winston": "^3.8.1",
    "word-list": "^3.1.0"
  },
  "engineStrict": true,
  "engines": {
    "node": "16.15.1",
    "npm": "8.11.0"
  }
}
