{
  "env": {
    "es6": true,
    "node": true
  },
  "extends": [
    "@codeyourfuture/standard",
    "plugin:jest/recommended"
  ],
  "globals": {
    "defer": false,
    "tick": false
  },
  "parser": "babel-eslint",
  "rules": {
    "jest/expect-expect": "off",
    "jest/no-focused-tests": "error",
    "jest/no-disabled-tests": "error"
  }
}
